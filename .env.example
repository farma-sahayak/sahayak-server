# Sahayak Environment Configuration

# Environment
ENVIRONMENT=development
DEBUG=true
LOG_LEVEL=INFO

# Google Cloud
GOOGLE_CLOUD_PROJECT=your-project-id
GOOGLE_APPLICATION_CREDENTIALS=./keys/service-account.json

# Firebase
FIREBASE_PROJECT_ID=your-firebase-project
FIREBASE_API_KEY=your-firebase-api-key

# API Keys
GEMINI_API_KEY=your-gemini-api-key
WEATHER_API_KEY=your-weather-api-key

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/sahayak
REDIS_URL=redis://localhost:6379

# Pub/Sub
AGENT_INVOCATIONS_TOPIC=agent-invocations
CROP_AGENT_SUBSCRIPTION=crop-agent-subscription

# Development
PORT=8000
CROP_AGENT_PORT=8080
